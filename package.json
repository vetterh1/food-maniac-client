    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "redux-devtools": "^3.3.1",
    "request": "^2.79.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-shell-plugin": "^0.5.0"
  }
}
