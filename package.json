{
  "name": "food-maniac",
  "version": "0.5.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "BoServer:dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development SERVER_NAME=BoServer nodemon server/boServer.js --ignore 'server/static_dev/' --exec \"babel-node --presets es2015,stage-2\"",
    "BoServer:dev:insertInitialData": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development SERVER_NAME=BoServer INSERT_INITIAL_DATA=true nodemon server/boServer.js --ignore 'server/static_dev/' --exec \"babel-node --presets es2015,stage-2\"",
    "BoServer:build:prod": "babel server -d distBoServer --presets es2015,stage-2 --copy-files",
    "BoServer:prod": "npm run BoServer:build:prod && cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production SERVER_NAME=BoServer nodemon distBoServer/boServer.js",
    "BoServer:prod:insertInitialData": "npm run BoServer:build:prod && cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production SERVER_NAME=BoServer INSERT_INITIAL_DATA=true nodemon distBoServer/boServer.js",
    "BoServer:prod:node": "cross-env NODE_ENV=production SERVER_NAME=BoServer node distBoServer/boServer.js",
    "BoServer:test": "cross-env  SERVER_NAME=BoServer NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register,css:test/mocha_ignore.js,png:test/mocha_ignore.js -d test/**/*.js",
    "BoServer:testVerbose": "./node_modules/.bin/mocha --compilers js:babel-register --reporter spec",
    "BoServer:lint": "eslint server",
    "FoServer:dev": "npm run FoServer:build:dev && cross-env NODE_ENV=development SERVER_NAME=FoServer webpack-dev-server --progress --colors --inline --content-base distFoServer --history-api-fallback",
    "FoServer:build:dev": "npm run FoServer:build:misc && cross-env NODE_ENV=development  SERVER_NAME=FoServer webpack",
    "FoServer:build:prod": "npm run FoServer:build:server && npm run FoServer:build:misc && cross-env NODE_ENV=production  SERVER_NAME=FoServer webpack",
    "FoServer:prod": "npm run FoServer:build:prod && cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production  SERVER_NAME=FoServer nodemon distFoServer/foServer.js",
    "FoServer:prod:node": "cross-env NODE_ENV=production SERVER_NAME=FoServer node distFoServer/foServer.js",
    "FoServer:testBuildWithWebpack": "webpack-dev-server --progress --colors --inline --content-base distFoServer --history-api-fallback",
    "FoServer:build:server": "babel server -d distFoServer --presets es2015,stage-2 --copy-files",
    "FoServer:build:misc": "npm run FoServer:build:cp_css && npm run FoServer:build:cp_images",
    "FoServer:build:mkdir": "if-env OS=Windows_NT && mkdir distFoServer\\images || mkdir distFoServer/images",
    "FoServer:build:cp_images": "if-env OS=Windows_NT && copy .\\app\\images\\*.* distFoServer\\images || cp ./app/images/*.* distFoServer/images",
    "FoServer:build:cp_css": "if-env OS=Windows_NT && copy .\\app\\index.css distFoServer || cp ./app/index.css distFoServer",
    "FoServer:test": "jest",
    "FoServer:test:watch": "jest --watch",
    "FoServer:test:coverage": "jest --watch --coverage",
    "lint": "eslint client server"
  },
