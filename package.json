  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register -R spec",
    "testOld": "./node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:dev": "npm run build && webpack-dev-server --inline --content-base dist --history-api-fallback",
    "start:prod": "npm run build && node server/server.js",
    "build": "npm run build:cp_css && webpack",
    "build:cp_css": "if-env OS=Windows_NT && copy .\\app\\index.css dist || cp ./app/index.css dist",
    "pm2:restart": "pm2 restart maptest",
    "start:old": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev"
  },
