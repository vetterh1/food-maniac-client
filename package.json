{
  "name": "food-maniac",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register,css:test/mocha_ignore.js,png:test/mocha_ignore.js -d test/**/*.js",
    "testOld": "./node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec",
    "start": "npm run build && webpack-dev-server --progress --colors --inline --content-base dist --history-api-fallback",
    "start:devnodeKO": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production npm run build && node server/server.js",
    "build": "npm run build:cp_css && npm run build:cp_images && webpack",
    "build:total": "npm run build:cp_css && npm run build:create_images && npm run build:cp_images && webpack",
    "build:cp_images": "if-env OS=Windows_NT && copy .\\app\\images\\*.* dist || cp ./app/images/*.* dist",
    "build:create_images": "mkdir .\\app\\images",
    "build:cp_css": "if-env OS=Windows_NT && copy .\\app\\index.css dist || cp ./app/index.css dist",
    "pm2:restart": "pm2 restart maptest",
    "start:old": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "lint": "eslint client server",
    "webpack:test": "webpack --display-error-details",
    "bn": "./node_modules/babel-cli/bin/babel-node.js",
    "babel": "babel-node"
  },
