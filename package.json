    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "file-stream-rotator": "0.0.7",
    "formsy-material-ui": "^0.5.3",
    "formsy-react": "^0.19.0",
    "fs": "0.0.1-security",
    "http-server": "^0.9.0",
    "if-env": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "loglevel": "^1.4.1",
    "material-ui": "^0.16.4",
    "mongoose": "^4.7.1",
    "morgan": "^1.7.0",
    "radium": "^0.18.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-fontawesome": "^1.4.0",
    "react-geosuggest": "^2.0.0",
    "react-rating": "^0.6.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-transition": "0.0.6",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "sanitize-html": "^1.13.0",
    "winston": "^2.2.0"
