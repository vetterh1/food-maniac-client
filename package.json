{
  "name": "tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "copy .\\app\\index.css dist && webpack",
    "build:prodWin": "copy .\\app\\index.css dist && set NODE_ENV=production&&webpack", 
    "build:prod": "cp ./app/index.css dist && NODE_ENV=production&&webpack", 
    "start": "copy .\\app\\index.css dist && webpack-dev-server --inline --content-base dist --history-api-fallback",
    "start:prodWin": "npm run build:prodWin && node server.js",
    "start:prod": "npm run build:prod && node server.js",
    "start:pm2": "npm run build:prod && restart maptest",

    "build2:server": "webpack --config webpack.server.config.js",
    "start2:prodWin": "npm run build2:server && npm run build:prodWin && node server.bundle.js",
    "start2:prod": "npm run build2:server && npm run build:prod && node server.bundle.js",
    "start2:pm2": "npm run build2:server && npm run build:prod && restart maptest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.8.5",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
